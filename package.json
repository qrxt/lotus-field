{
  "name": "lotus-field",
  "version": "1.0.0",
  "description": "mtg wishlist app",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.prod.babel.js",
    "build-dev": "webpack --mode development --config webpack.config.dev.babel.js",
    "watch": "webpack-dev-server --mode development --config webpack.config.dev.babel.js",
    "bundle-report": "webpack-bundle-analyzer --port 8888 dist/stats.json",
    "lint-js": "eslint ./src/ --ext .js --ext .jsx",
    "lint-css": "stylelint ./src/**/*.css --config .stylelintrc.json",
    "test": "jest --passWithNoTests --bail"
  },
  "author": "qrx",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.css": [
      "npm run lint-css"
    ],
    "./src/**/*.{js,jsx}": [
      "npm run lint-js",
      "npm test"
    ]
  },
  "devDependencies": {
